

<div class="row">

<% @videos.each do |video|%>
      <div class="col-md-3">
      <%= link_to  video.title, video_path(video) %>
      <iframe width="100%" height="100%" src="https://www.youtube.com/embed/<%= video.youtube_id%>" frameborder="0" allowfullscreen></iframe>
      </div>
<% end %>
</div>